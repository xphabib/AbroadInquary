<% @users.each do |user| %>
  <%= user.id %>
  <%= user.email %>
  <%= user.full_name %>
  <%= user.phone %>
  <%= user.city.name if user.city.present?%>
  <%= user.country_name %>
  <%= user.department_name %>
  <%= user.university %>
  <%= user.cgpa %>
  <%= user.nationality %>
  <%= user.occupation %>
  <%= user.user_type %>
  <%= user.role %>
  <%= user.image %>
  <%= link_to 'Show', user_path(user) %>
  <%= link_to 'Edit', edit_user_path(user) %>
  <%= link_to 'Delete', user_path(user), method: :delete, data: {confirm: 'are you sure'} %>
<% end %>
<%= link_to 'new_user', new_user_path %>