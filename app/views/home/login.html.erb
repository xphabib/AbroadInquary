<section class="page-heading-section before-bg bg-image" style="background-image: url(<%= asset_path('header/p-header3.jpg') %>);">
  <div class="container">
    <div class="row">
      <div class="col-md-12 col-sm-12">
        <div class="page-heading">
          <h2 class="page-name text-capitalize">login</h2>
        </div><!--/.page-heading-->
      </div><!--/.col-md-12-->
    </div><!--/.row-->
  </div><!--/.container-->
</section><!--/.page-heading-area-->


<div class="login-page page-wrapper s-pd100">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-lg-4 col-md-6 col-sm-8">
        <div class="login-form-area">
          <form action="http://bdevs.net/edulab/edulab/post">
            <h2>login</h2>
            <div id="socila-login" class="social-login-area">
              <a class="facebook" href="login.html#"><i class="fa fa-facebook"></i></a>
              <a class="twitter" href="login.html#"><i class="fa fa-twitter"></i></a>
              <a class="google" href="login.html#"><i class="fa fa-google-plus"></i></a>
              <a class="linkedin" href="login.html#"><i class="fa fa-linkedin"></i></a>
            </div>
            <div class="divider-or text-center">
              <span>or</span>
            </div>
            <p><input class="form-control" type="text" name="log" id="user_login" placeholder="Username / Email"></p>
            <p><input class="form-control" type="password" name="pwd" id="user_pass" placeholder="Password"></p>
            <a class="login-form-forgot-password" href="login.html#">Forgot your password?</a>
            <p><button class="btn btn-default btn-primary" type="submit">Log In Your Account</button></p>
            <div class="login-form-remember">
              <label><input type="checkbox" id="rememberme" value="">Keep me signed in until I sign out</label>
            </div>
            <div class="login-form-register-now"><p>Not yet a member? <a href="register.html">REGISTER NOW</a></p></div>
          </form>
        </div><!--/.login-form-area-->
      </div><!--/.col-lg-4-->
    </div><!--/.row-->
  </div><!--/.container-->
</div><!--/.login-page-->
